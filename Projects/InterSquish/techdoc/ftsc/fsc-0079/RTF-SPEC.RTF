\par \pard\plain \s3\sb240\sa240\keepn \b\f5\fs28 {\*\bkmkstart _Toc313960855}{\*\bkmkstart _Toc315156229}\page RTFREADR.C{\*\bkmkend _Toc313960855}{\*\bkmkend _Toc315156229}
\par \pard\plain \s69\li432 \f11\fs16 #include <stdio.h>
\par \pard \s69\li432 #include <stdlib.h>
\par #include <ctype.h>
\par #include "rtftype.h"
\par #include "rtfdecl.h"
\par 
\par int cGroup;
\par bool fSkipDestIfUnk;
\par long cbBin;
\par long lParam;
\par 
\par RDS rds;
\par RIS ris;
\par 
\par CHP chp;
\par PAP pap;
\par SEP sep;
\par DOP dop;
\par 
\par SAVE *psave;
\par FILE *fpIn;
\par 
\par //
\par // %%Function: main
\par //
\par // Main loop. Initialize and parse RTF.
\par //
\par main(int argc, char *argv[])
\par \{
\par     FILE *fp;
\par     int ec;
\par 
\par     fp = fpIn = fopen("test.rtf", "r");
\par     if (!fp)
\par     \{
\par         printf ("Can't open test file!\\n");
\par         return 1;
\par     \}
\par     if ((ec = ecRtfParse(fp)) != ecOK)
\par         printf("error %d parsing rtf\\n", ec);
\par     else
\par         printf("Parsed RTF file OK\\n");
\par     fclose(fp);
\par     return 0;
\par \}
\par 
\par //
\par // %%Function: ecRtfParse
\par //
\par // Step 1:
\par // Isolate RTF keywords and send them to ecParseRtfKeyword;
\par // Push and pop state at the start and end of RTF groups;
\par // Send text to ecParseChar for further processing.
\par //
\par 
\par int
\par ecRtfParse(FILE *fp)
\par \{
\par     int ch;
\par     int ec;
\par     int cNibble = 2;
\par     int b = 0;
\par     while ((ch = getc(fp)) != EOF)
\par     \{
\par         if (cGroup < 0)
\par             return ecStackUnderflow;
\par         if (ris == risBin)                      // if we're parsing binary data, handle it directly
\par         \{
\par             if ((ec = ecParseChar(ch)) != ecOK)
\par                 return ec;
\par         \}
\par         else
\par         \{
\par             switch (ch)
\par             \{
\par             case '\{':
\par                 if ((ec = ecPushRtfState()) != ecOK)
\par                     return ec;
\par                 break;
\par             case '\}':
\par                 if ((ec = ecPopRtfState()) != ecOK)
\par                     return ec;
\par                 break;
\par             case '\\\\':
\par                 if ((ec = ecParseRtfKeyword(fp)) != ecOK)
\par                     return ec;
\par                 break;
\par             case 0x0d:
\par             case 0x0a:          // cr and lf are noise characters...
\par                 break;
\par             default:
\par                 if (ris == risNorm)
\par                 \{
\par                     if ((ec = ecParseChar(ch)) != ecOK)
\par                         return ec;
\par                 \}
\par                 else
\par                 \{               // parsing hex data
\par                     if (ris != risHex)
\par                         return ecAssertion;
\par                     b = b << 4;
\par                     if (isdigit(ch))
\par                         b += (char) ch - '0';
\par                     else
\par                     \{
\par                         if (islower(ch))
\par                         \{
\par                             if (ch < 'a' || ch > 'f')
\par                                 return ecInvalidHex;
\par                             b += (char) ch - 'a';
\par                         \}
\par                         else
\par                         \{
\par                             if (ch < 'A' || ch > 'F')
\par                                 return ecInvalidHex;
\par                             b += (char) ch - 'A';
\par                         \}
\par                     \}
\par                     cNibble--;
\par                     if (!cNibble)
\par                     \{
\par                         if ((ec = ecParseChar(ch)) != ecOK)
\par                             return ec;
\par                         cNibble = 2;
\par                         b = 0;
\par ris = risNorm;
\par                     \}
\par                 \}                   // end else (ris != risNorm)
\par                 break;
\par             \}       // switch
\par         \}           // else (ris != risBin)
\par     \}               // while
\par     if (cGroup < 0)
\par         return ecStackUnderflow;
\par     if (cGroup > 0)
\par         return ecUnmatchedBrace;
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecPushRtfState
\par //
\par // Save relevant info on a linked list of SAVE structures.
\par //
\par 
\par int
\par ecPushRtfState(void)
\par \{
\par     SAVE *psaveNew = malloc(sizeof(SAVE));
\par     if (!psaveNew)
\par         return ecStackOverflow;
\par 
\par     psaveNew -> pNext = psave;
\par     psaveNew -> chp = chp;
\par     psaveNew -> pap = pap;
\par     psaveNew -> sep = sep;
\par     psaveNew -> dop = dop;
\par     psaveNew -> rds = rds;
\par     psaveNew -> ris = ris;
\par     ris = risNorm;
\par     psave = psaveNew;
\par     cGroup++;
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecPopRtfState
\par //
\par // If we're ending a destination (that is, the destination is changing),
\par // call ecEndGroupAction.
\par // Always restore relevant info from the top of the SAVE list.
\par //
\par 
\par int
\par ecPopRtfState(void)
\par \{
\par     SAVE *psaveOld;
\par     int ec;
\par 
\par     if (!psave)
\par         return ecStackUnderflow;
\par 
\par     if (rds != psave->rds)
\par     \{
\par         if ((ec = ecEndGroupAction(rds)) != ecOK)
\par             return ec;
\par     \}
\par     chp = psave->chp;
\par     pap = psave->pap;
\par     sep = psave->sep;
\par     dop = psave->dop;
\par     rds = psave->rds;
\par     ris = psave->ris;
\par 
\par     psaveOld = psave;
\par     psave = psave->pNext;
\par     cGroup--;
\par     free(psaveOld);
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecParseRtfKeyword
\par //
\par // Step 2:
\par // get a control word (and its associated value) and
\par // call ecTranslateKeyword to dispatch the control.
\par //
\par 
\par int
\par ecParseRtfKeyword(FILE *fp)
\par \{
\par     int ch;
\par     char fParam = fFalse;
\par     char fNeg = fFalse;
\par     int param = 0;
\par     char *pch;
\par     char szKeyword[30];
\par     char szParameter[20];
\par 
\par     szKeyword[0] = '\\0';
\par     szParameter[0] = '\\0';
\par     if ((ch = getc(fp)) == EOF)
\par         return ecEndOfFile;
\par     if (!isalpha(ch))           // a control symbol; no delimiter.
\par     \{
\par         szKeyword[0] = (char) ch;
\par         szKeyword[1] = '\\0';
\par         return ecTranslateKeyword(szKeyword, 0, fParam);
\par     \}
\par     for (pch = szKeyword; isalpha(ch); ch = getc(fp))
\par         *pch++ = (char) ch;
\par     *pch = '\\0';
\par     if (ch == '-')
\par     \{
\par         fNeg  = fTrue;
\par         if ((ch = getc(fp)) == EOF)
\par             return ecEndOfFile;
\par     \}
\par     if (isdigit(ch))
\par     \{
\par         fParam = fTrue;         // a digit after the control means we have a parameter
\par         for (pch = szParameter; isdigit(ch); ch = getc(fp))
\par             *pch++ = (char) ch;
\par         *pch = '\\0';
\par         param = atoi(szParameter);
\par         if (fNeg)
\par             param = -param;
\par         lParam = atol(szParameter);
\par         if (fNeg)
\par             param = -param;
\par     \}
\par     if (ch != ' ')
\par         ungetc(ch, fp);
\par     return ecTranslateKeyword(szKeyword, param, fParam);
\par \}
\par 
\par //
\par // %%Function: ecParseChar
\par //
\par // Route the character to the appropriate destination stream.
\par //
\par 
\par int
\par ecParseChar(int ch)
\par \{
\par     if (ris == risBin && --cbBin <= 0)
\par         ris = risNorm;
\par     switch (rds)
\par     \{
\par     case rdsSkip:
\par         // Toss this character.
\par         return ecOK;
\par     case rdsNorm:
\par         // Output a character. Properties are valid at this point.
\par         return ecPrintChar(ch);
\par     default:
\par     // handle other destinations....
\par         return ecOK;
\par     \}
\par \}
\par 
\par //
\par // %%Function: ecPrintChar
\par //
\par // Send a character to the output file.
\par //
\par 
\par int
\par ecPrintChar(int ch)
\par \{
\par     // unfortunately, we don't do a whole lot here as far as layout goes...
\par     putchar(ch);
\par     return ecOK;
\par \}
